<?xml version='1.0' encoding='utf-8'?>
<widget id="com.navigate.thegame" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <allow-navigation href="cdvfile:*" />
    <feature name="FileTransfer">
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
    </feature>
    <feature name="Insomnia">
        <param name="android-package" value="nl.xservices.plugins.Insomnia" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FirebasePlugin">
        <param name="android-package" value="org.apache.cordova.firebase.FirebasePlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="XAPKReader">
        <param name="android-package" value="com.flyingsoftgames.xapkreader.XAPKReader" />
        <param name="onload" value="true" />
    </feature>
    <name>The Game</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <resource-file src="google-services.json" target="google-services.json" />
    <icon density="ldpi" src="../assets/img/icons/icon_36x36.png" />
    <icon density="mdpi" src="../assets/img/icons/icon_48x48.png" />
    <icon density="hdpi" src="../assets/img/icons/icon_72x72.png" />
    <icon density="xhdpi" src="../assets/img/icons/icon_96x96.png" />
    <icon density="xxhdpi" src="../assets/img/icons/icon_144x144.png" />
    <icon density="xxxhdpi" src="../assets/img/icons/icon_192x192.png" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="android-minSdkVersion" value="21" />
</widget>
